
ff-canvas {
  width: 750rpx;
  height: 900rpx;
}
.month_select{
    padding: 0 30rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    height: 58rpx;
    align-items: center;
    margin: 34rpx 0;
    width:690rpx;

}
.month_select text{
    font-size:24rpx;
    color:#1284EC;
}

.month_select .month{
    width: 560rpx;
    border:1px solid rgba(195,193,193,1);
border-radius:5px;
box-sizing: border-box;
position: relative;
text-align: center;
height: 58rpx;
line-height: 58rpx;
font-size:24rpx;
font-family:PingFang-SC-Medium;
font-weight:500;
color:rgba(51,51,51,1);
}


